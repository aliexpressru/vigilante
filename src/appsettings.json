{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Vigilante": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Vigilante": "Information"
      }
    }
  },
  "Qdrant": {
    "MonitoringIntervalSeconds": 30,
    "HttpTimeoutSeconds": 5,
    "EnableAutoRecovery": true,
    "ApiKey": "test",
    "Nodes": [
      {
        "Host": "localhost",
        "Port": 6333
      },
      {
        "Host": "localhost",
        "Port": 6343
      },
      {
        "Host": "localhost",
        "Port": 6353
      }
    ],
    "S3": {
      "BucketName": "snapshots"
    }
  },
  "AllowedHosts": "*"
}
